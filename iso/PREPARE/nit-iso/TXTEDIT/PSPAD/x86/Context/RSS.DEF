<?xml version="1.0"?>

<!DOCTYPE rss SYSTEM "http://my.netscape.com/publish/formats/rss-0.91.dtd">
<!-- RSS generated by PSPad editor, www.pspad.com; %RFCTime% -->
<rss version="0.91">
  <channel>
    <language>%language%</language>
    <description></description>
    <link></link>
    <title></title>
    <image>
      <link></link>
      <title></title>
      <url></url>
    </image>
  </channel>
</rss>
