[Macro definition]
%alt%=@E alt=,alt=,,,'
%border%=@E border=,border=,0,,'
%class%=@C class=,class=,,annex;answer;author;board;comment;definition;extra;kniha-popis;list;mouse;note-author;prepend;question;refer;sample;table;text,'
%height%=@E height=,height=,,,'
%href%=@O HREF=,href=,,html file (*.html;*.htm;*.php;*.php4;*.asp)|*.html;*.htm;*.php;*.php4;*.asp,'
%nazevclanku%=@E Název článku:
%poutac%=@E Poutač:
%src%=@P src=,src=,,,'
%title%=@E title=,title=,,,'
%width%=@E width=,width=,,,'
;
[&nbsp; |G nedělitelná mezera]*Shift+Ctrl+Space
&nbsp;
[&bdquo; |G uvozovka dole (99)]
&bdquo;
[&ldquo; |G uvozovka nahoře (66)]*Ctrl+U
&ldquo;
[9966 |G text do uvozovek (9966)]
&bdquo;§|&ldquo;
[&apos; |G apostrof]
&apos;
[&gt; |G znak ">"]
&gt;
[&lt; |G znak "<"]
&lt;
[&amp; |G ampersand]
&amp;
;
[<...> |G začátek/konec tagu z označeného textu]*Ctrl+,
<§>|</§>
[<> |G začátek tagu <>]*Alt+,
<|>
[</> |G konec tagu </|>]*Alt+.
</|>
;
[!-- |G řádkový komentář <!-- text -->]
<!-- §| -->
[!--- |G komentář bloku <!-- text -->]
<!--
 §|
-->
