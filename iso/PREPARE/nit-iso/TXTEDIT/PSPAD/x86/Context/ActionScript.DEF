;---------------------------------------------------------------------
;  tagy HTML version 1.0, kterou Flash podporuje v dynamických textových polích
;---------------------------------------------------------------------
;
[a |B odkaz bez podtržení]
<a %href% %target%>§|</a>
;
[a |B odkaz s podtržením]
<u><a %href%>§|</a></u>
;
[b |B tučné písmo <b></b>]*CTRL+B
<b>§|</b>
;
[body |B tělo dokumentu]
<body>
  §|
</body>
;
[br |B zalomení řádku]*CTRL+ENTER
<br>
;
[font |B nastavení písma]
<font %size% %face% %color%>§|</font>
;
[html |B vymezení dokumentu <html></html>]
<html>
  §|
</html>
;
[HTMLs |B jednoduchá šablona HTML stránky]
%variable%=<html>
 <body>
  §|
 </body>
</html>
;
[i |B kurzíva]
<i>§|</i>
;
[p |B odstavec]*SHIFT+CTRL+P
<p %align3%>
  §|
</p>
;
[u |B podtržený text]
<u>§|</u>
;
;
;---------------------------------------------------------------------
;  ActionScript
;---------------------------------------------------------------------
;
;
[/*...*/ | příkaz comment pro víceřádkový komentář]
/*
  §|
*/
;
[break | příkaz k přerušení cyklu]
break;
;
[continue  | ukončí krok cyklu a začne od nového kroku nebo od návěstí]
continue;
;
[call  |D provede skript připojený k určitému návěští snímku]
call(%frame%);
;
[delete  | odstraní proměnnou]
delete %variable%;
;
[do_while | smyčka provádí příkazy do splnění určené podmínky]
do {
  |
} while (%condition%)
;
[else | určuje příkazy, které se provedou pokud není splněna podmínka if]
elese {
	|
}
;
[exIMAGE  |N načtení externího obrázku do prezentace]
createEmptyMovieClip(%variable%, %n%);
%variable%.loadMovie(%loadi%);
%variable%._visible = true;
%variable%._x=%x%;
%variable%._y=%y%;
;
[for | vytvoří cyklus, který provádí příkazy do splnění podmínky]
for (%init%; %condition%; %next%){
	|
}
;
[fscommand | FS příkaz]
fscommand(%command%, %boolean%);
;
[fscommandA | zabránění změny velikosti prezentace]
fscommand("allowscale","false");
;
[fscommandF | puštění prezentace na celou obrazovku]
fscommand("fullscreen","true");
;
[fscommandN | otevření dokumentu *.txt v Notepadu z Flash prezentace]
fscommand("exep","notepad"+chr(9)+%loadt%);
;
[fscommandQ | ukončení prezentace]
fscommand("quit");
;
[fscommandS | zneviditelnění kontextového menu]
fscommand("showmenu","false");
;
[fscommandT | zakázaní klávesových zkratek]
fscommand("trapallkeys","true");
;
[getURL | načte soubour na určeném URL]
getURL(%url%);
;
[gotoAndPlay | přejít a spustit]
gotoAndPlay(|);
;
[gotoAndStop | přejít a zastavit]
gotoAndStop(|);
;
[if | definování příkazů, které se provedou, pokud je splněná podmínka]
if (%condition%) {
	 |
}
;
[ifFrameLoaded |D ]
ifFrameLoaded()
;
[include | vloží ActionScriptu z externího textového dokumentu]
#include "%include%"
;
[initclip | inicializace]
#initclip |
;
[loadMovie | načíst movie]
loadMovie(%url%, %method%);
;
[loadMovieNum | načíst movie s určením úrovně]
loadMovie(%url%, %level%);
;
[loadVariables | načte do movie clipu externí proměnou z textového souboru]
loadVariables(%loadt%, %method%);
;
[Mouse.hide | skryje kurzor v animaci]
Mouse.hide();
;
[Mouse.show | zobrazí kurzor v animaci]
Mouse.show();
;
[nextScene | další scéna]
nextScene();
;
[nextFrame | následující snímek]
nextFrame();
;
[on | přiřazení akce tlačítku]
on (%on%) {
}
;
[on1 |N přechod na jinou scénu po stisknutí tlačítka]
on(release) {
  tellTarget (_root) {
    gotoAndPlay (%scene%, %frame%)
  }
}
;
[onJS |N odkaz na JavaScript]
on (release) {
  getURL("Javascript:|");
}
;
[onClipEvent | při události klipu]
onClipEvent(%event%) {
}
;
[play | začne přehrávat časovou osu movie clipu z aktuální pozice]
play();
;
[prevScene | předcházející scéna]
prevScene();
;
[useCodepage | povolení používání znakové sady Unicode k zajištění správného zobrazování písmen s diakritikou]
System.useCodepage=true;
;
[setDate | nastaví den v měsíci]
setDate(|);
;
[setFullYear | nastaví měsíc, rok a datum]
setFullYear(|);
;
[stop | zastavit přehrávání]
stop();
;
[stopAllSounds | zastaví přehrávání všech zvuků]
stopAllSounds();
;
[stopDrag | ukončí následování kurzoru myši movie clipem]
stopDrag();
;
[targetPath | ]
targetPath();
;
[toggleHighQuality |D ]
toggleHighQuality()
;
[trace | v testovacím módu zobrazí výraz v okně Output]
trace("§|");
;
[undefined | nedefinováno]
undefined
;
[unloadMovie | odstranit načtenou movie]
unloadMovie(%url%|);
;
[unloadMovieNum | odstranit načtenou movie z úrovně]
unloadMovieNum(%level%);
;
[var | definuje lokální proměnné obsažené uvnitř funkce]
var %variable% = %value%;
;
[XML | vytvoření instance XML objektu]
%XML% = new XML();
;
[XMLattributes | reprezentuje atribut v objektu XMLNode]
%XML%.attributes;
;
[XMLload | ]
%XML%.load(%loadxml%);
;
[XMLloaded | ověření dokončení procesu vkládání XML dokumentu]
%XML%.loaded;
;
[XMLignoreWhite| ignorace bílých znaků]
%XML%.ignoreWhite = true;
;
[XMLonData | ]
%XML%.onData(%xmlsrc%);
;
[XMLonLoad | ]
%XML%.onLoad(success);
;
[XMLNode.parentNode | vrací rodičovský nód referenčního nódu]
.parentNode;
;
[XMLsend | odeslání XML objektu]
%XML%.send(%url%, %window%);
;
[XMLxmlDec1 | vrací XML deklaraci XML]
%XML%.xmlDec1|
;
